{
  "name": "@magnite/fledge.polyfill",
  "homepage": "https://github.com/MagniteEngineering/fledge.polyfill#readme",
  "description": "A Fledge JavaScript polyfill",
  "author": "Newton Koumantzelis",
  "version": "0.3.2",
  "main": "dist/api/cjs/index.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "fledge",
    "turtledove",
    "sparrow",
    "dovekey",
    "parrrot",
    "tern",
    "advertising",
    "w3c web advertising business group"
  ],
  "repository": "https://github.com/MagniteEngineering/fledge.polyfill.git",
  "bugs": "https://github.com/MagniteEngineering/fledge.polyfill/issues",
  "releases": "https://github.com/MagniteEngineering/fledge.polyfill/releases",
  "wiki": "https://github.com/MagniteEngineering/fledge.polyfill/wiki",
  "license": "MIT",
  "alex": {
    "allow": [
      "color",
      "colors",
      "dead",
      "disabled",
      "invalid"
    ]
  },
  "engines": {
    "node": "^14.x"
  },
  "jest": {
    "projects": [
      {
        "resetMocks": false,
        "collectCoverage": true,
        "collectCoverageFrom": [
          "<rootDir>/src/**/*.{js,jsx}",
          "!<rootDir>/**/{__mocks__, mocks}/**",
          "!<rootDir>/**/*.{mocks,mock}.js",
          "!<rootDir>/**/{__stories__,stories,story}/**",
          "!<rootDir>/**/{__tests__,tests,test}/__snapshots__/**"
        ],
        "coverageThreshold": {
          "global": {
            "statements": 80,
            "branches": 80,
            "lines": 80,
            "functions": 80
          }
        },
        "displayName": "Unit Tests",
        "modulePathIgnorePatterns": [
          "<rootDir>/dist/"
        ],
        "testRegex": [
          "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
          "(/tests?/.*|(\\.|/)(test|spec))\\.[jt]sx?$"
        ],
        "testPathIgnorePatterns": [
          "/coverage/",
          "/node_modules/",
          "((\\.|/)(mocks|mock))\\.[jt]sx?$"
        ],
        "transform": {
          "^.+\\.js$": "babel-jest"
        },
        "transformIgnorePatterns": [
          "<rootDir>/node_modules/"
        ],
        "setupFiles": [
          "fake-indexeddb/auto",
          "jest-localstorage-mock"
        ]
      },
      {
        "displayName": "End-to-End Integration Tests",
        "setupFiles": [
          "fake-indexeddb/auto"
        ],
        "testMatch": [
          "<rootDir>/test/e2e/**/*.js"
        ]
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "predist": "rollup -c rollup.config.js --input=src/frame --output=dist/frame --name=fledgeframe",
    "dist": "rollup -c rollup.config.js --input=src/api --output=dist/api --name=fledge",
    "predocs": "rollup -c rollup.config.js --input=src/frame --output=docs/scripts/frame --name=fledgeframe",
    "docs": "rollup -c rollup.config.js --input=src/api --output=docs/scripts/api --name=fledge",
    "lint": "eslint .",
    "release": "semantic-release",
    "serve": "serve . -l 3000",
    "start": "serve docs -l 8000",
    "test": "npm run test:docs && npm run test:unit && npm run test:e2e",
    "test:docs": "alex .",
    "pretest:e2e": "npm run dist",
    "test:e2e": "jest --config test/e2e/jest.config.js --runInBand",
    "test:unit": "jest test/unit --passWithNoTests",
    "test:perf": "serve . -l 3000 & export APP_PID=$! && node test/perf/index.js; kill -9 $APP_PID"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-strip": "^2.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "alex": "^9.1.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "26.6.3",
    "csv-writer": "^1.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "fake-indexeddb": "^3.1.2",
    "fibonacci": "^1.6.7",
    "jest": "25.1.0",
    "jest-localstorage-mock": "^2.4.10",
    "jest-puppeteer": "^6.0.0",
    "microtime": "^3.0.0",
    "puppeteer": "^9.1.0",
    "randomstring": "^1.1.5",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.2",
    "serve": "^12.0.0"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "idb-keyval": "^5.0.5"
  }
}
